<?xml version='1.0' encoding='utf-8'?>
<!-- Full info on config files: https://cordova.apache.org/docs/en/latest/config_ref/index.html -->
<widget android-versionCode="1" id="kemi.babytoyphone.app" ios-CFBundleVersion="1" version="1.0.0"
    xmlns="https://www.w3.org/ns/widgets">
    <name>baby toy phone</name>
    <description>A phone toy app for baby education and entertainment.</description>
    <icon src="/resources/android/icon/icon.png" />
      
     <content src="index.html" />
    <preference name="Orientation" value="landscape" />
    <preference name="permissions" value="media" />
    <preference name="permissions" value="network access" />
    <plugin name="cordova-plugin-statusbar" />
    <plugin name="cordova-plugin-admobpro" spec='8.13.1' />
    <preference name="AndroidXEnabled" value="true" />
    <allow-navigation href="*" />
    <access origin="*" />
    <allow-intent href="*" />
    <preference name="android-minSdkVersion" value="22"/>
    <engine name="android" spec="11.0.0" />
   <!-- Images can also be generated by cordova-res -->
  </widget>
